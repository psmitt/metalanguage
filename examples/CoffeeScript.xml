<syntax language="CoffeeScript" start="Source">
  <rule name="Source">
    <definition>
      <optional>
        <nonterminal name="Body"/>
      </optional>
    </definition>
  </rule>
  <rule name="Body">
    <definition>
      <nonterminal name="Line"/>
      <repeated>
        <optional>
